.download-btn span{background: url("../images/download.png") no-repeat left center;background-size: 22px 22px; padding-left: 30px;color: #fff;}

.imglists{width: 100%;border: 1px dashed #cfcfcf;height: 85px; position: relative;margin-top: 5px;}
.imglist{width: 100%;height: 85px;line-height: 83px;text-align: center;color: #999;display: inline-block;}
.imglist img{margin-right: 8px;}
.imglists input[type=file]{position: absolute;left: 0;top: 0;bottom:0;height:100% !important; width: 100%;overflow: hidden;opacity: 0;filter:alpha(opacity=0);display:block;cursor: pointer;}

.reg-file-img{border: 1px dashed #ccc;background: #fff;overflow: hidden;height: 90px;position: relative;}
.reg-file-img img{width: 100%; height: 100%;line-height: 90px;}
.reg-file-img input[type=file]{filter:alpha(opacity=0);-moz-opacity:0;-khtml-opacity: 0; opacity: 0;width: 100%; height: 90px !important;cursor: pointer;position:absolute;top: 0;left: 0;z-index: 99;}
.reg-file-img span{background: url("../../images/icon2.png") no-repeat top 35px left 20px;background-size: 20px;padding-left:30px;color: #999;height: 22px;display: inline-block;position:absolute;top: 0;left: 0;height: 90px;line-height: 90px;z-index: 1;padding-left: 30px;text-align: center;width: 100%;}
.reg-file .font{font-weight:400;}

.imgMid{width:100%;padding-top:8rem;}
.imgMid img{display:block;width:10rem;margin:0 auto;}
.textImg{text-align:center;padding-top:2rem;color:#666;}

/*增票资质*/
.zpzz{background: #e8f9fe;}
.wl-title-t{border-left: 4px solid #31cbf9;height: 20px; line-height: 20px; padding-left: 10px;}
.zpzz-list,.zpzz-list li{list-style: none;margin:0;padding: 0;}
.zpzz-list li{clear: both;border-bottom: 1px solid #e1e1e1;padding: 0.6rem 1rem;width: 100%;float: left;}
.zpzz-list li span{max-width: 60%;}
.zpzzqrs span{text-indent: 2em;display: block;}

/*开票*/
.wlkp-list ul,.wlkp-list li{padding: 0;margin: 0;list-style: none;}
.wlkp-list li{border: 1px solid #e1e1e1; padding: 10px;width: 100%;float: left;border-bottom: none;}
.wlkp-list li:last-child{border-bottom: 1px solid #e1e1e1;}
.wlkp-list li a{color: #333;}

.wlkp-checkbox{width: 30px;float: left;margin-top: -10px;}
.wlkp-ccon{width: 70%;float: left;}
.wlkp-list-left{width: 100px;float: left;}
.wlkp-list-right{width: calc(100% - 100px);width:-webkit-calc(100% - 100px);
    width:-moz-calc(100% - 100px);
    width:calc(100% - 100px);float: left;}
